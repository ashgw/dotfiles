# ===================================================================
# Basic file attributes (Tokyo Night palette)
# bg: #1a1b26, fg: #c0caf5
# red:    #f7768e  orange: #ff9e64  yellow: #e0af68
# green:  #9ece6a  cyan:   #7dcfff  blue:   #7aa2f7  purple: #bb9af7
# ===================================================================

# Reset
RESET 0

# Intentionally leave NORMAL/FILE unset to inherit terminal defaults
# NORMAL 00
# FILE   00

# Core types
DIR 01;38;2;187;154;247                  # directories purple
LINK 01;38;2;125;207;255                 # symlinks cyan
MULTIHARDLINK 00
FIFO 48;2;26;27;38;38;2;224;175;104      # pipe: dark bg, yellow fg
SOCK 01;38;2;187;154;247                 # socket purple
DOOR 01;38;2;187;154;247                 # door (Illumos)
BLK  48;2;26;27;38;38;2;224;175;104;01   # block dev
CHR  48;2;26;27;38;38;2;224;175;104;01   # char dev
ORPHAN 48;2;26;27;38;38;2;247;118;142;01 # broken link red on dark bg
MISSING 00
SETUID 38;2;192;202;245;48;2;247;118;142 # fg soft, bg red
SETGID 38;2;26;27;38;48;2;224;175;104    # bg yellow
CAPABILITY 00
STICKY_OTHER_WRITABLE 38;2;26;27;38;48;2;158;206;106
OTHER_WRITABLE       38;2;187;154;247;48;2;158;206;106
STICKY               38;2;192;202;245;48;2;187;154;247
EXEC 01;38;2;158;206;106                 # executables green bold

